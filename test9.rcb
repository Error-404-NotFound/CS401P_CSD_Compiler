func nestedMatch() -> int {
    x:int;
    y:int;
    res:int;
    x = 1;
    y = 2;
    res = 0;
    
    match (x) {
        1 => {
            match (y) {
                2 => {
                    res = 12;
                }
                _ => {
                    res = 10;
                }
            }
        }
        _ => {
            res = 99;
        }
    }
    
    reply res;
}