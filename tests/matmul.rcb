func main() -> int {
    a: int[9] = {1, 2, 3, 4, 5, 6, 7, 8, 9};
    b: int[9] = {9, 8, 7, 6, 5, 4, 3, 2, 1};

    i: int = 0;
    result("The contents of matrix 1:\n");

    loop while (i < 9) {
        t: int;
        result(a[i]);
        result(" ");
        i = i + 1;
    }

    result("\n");
    result("The contents of matrix 2:\n");

    i = 0;
    loop while (i < 9) {
        t: int;
        result(b[i]);
        result(" ");
        i = i + 1;
    }

    c: int[9];

    row: int = 0;
    loop while (row < 3) {
        col: int = 0;

        loop while (col < 3) {
            sum: int = 0;
            k: int = 0;

            loop while (k < 3) {
                sum = sum + a[row * 3 + k] * b[k * 3 + col];
                k = k + 1;
            }

            c[row * 3 + col] = sum;
            col = col + 1;
        }

        row = row + 1;
    }

    result("\n");

    i = 0;
    loop while (i < 9) {
        result(c[i]);
        if ((i + 1) % 3 == 0) {
            result("\n");
        } else {
            result(" ");
        }
        i = i + 1;
    }

    reply 0;
}
