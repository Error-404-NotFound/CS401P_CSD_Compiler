class Person {
    public name: int;
    public age: int;
    
    func greet() -> int {
        result("Hello");
        reply 0;
    }
}

record Point {
    x: int;
    y: int;
}

func main() -> int {
    p: Person = make Person;
    p.name = 12;
    p.age = 30;
    p.greet();
    
    pt: Point = make Point;
    pt.x = 10;
    pt.y = 20;
    
    discard p;
    reply 0;
}